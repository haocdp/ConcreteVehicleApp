    <ImageView 
        android:id="@+id/CarIcon" 
        android:layout_width="20.0dip" 
        android:layout_height="20.0dip" 
        android:layout_marginLeft="5.0dip" 
        android:layout_marginTop="2.0dip" 
        android:layout_marginRight="5.0dip" 
        android:layout_marginBottom="10.0dip" 
        android:src="@drawable/c41" 
        android:layout_alignParentRight="true" 
        />

    <ImageView
        android:id="@+id/iv_cartype"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="8dp"
        android:background="@drawable/cl00001"
         />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="8dp"
        android:layout_toRightOf="@id/iv_cartype"
         >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/tv_carnumber"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:textColor="@color/carinfotextcolor"
                android:textSize="16.0dip"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="5dp"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Acc状态 :"
                android:textColor="@color/carinfotextcolor"
                android:textSize="14.0dip" />
            
            <TextView
                android:id="@+id/tv_accstate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0.5"
                android:text="123456"
                android:textColor="@color/carinfotextcolor" 
                android:textSize="14.0dip"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:text="速度:"
                android:textColor="@color/carinfotextcolor"
                android:textSize="14.0dip" />
            
            <TextView
                android:id="@+id/tv_speed"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0.5"
                android:text="123456"
                android:textColor="@color/carinfotextcolor" 
                android:textSize="14.0dip"/>
        </LinearLayout>
        
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="时间 :"
                android:textColor="@color/carinfotextcolor"
                android:textSize="14.0dip" />
            
            <TextView
                android:id="@+id/tv_inserttime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0.5"
                android:text="123456"
                android:textColor="@color/carinfotextcolor" 
                android:textSize="14.0dip"/>
            
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="定位 :"
                android:textColor="@color/carinfotextcolor"
                android:textSize="14.0dip" />
            
            <TextView
                android:id="@+id/tv_gpsgeoposition"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0.5"
                android:text="GPS"
                android:textColor="@color/carinfotextcolor" 
                android:textSize="14.0dip"/>

            
        </LinearLayout>
    </LinearLayout>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:background="@drawable/into_icon"
        android:layout_alignParentRight="true"
        android:layout_marginRight="8dp" 
        android:layout_centerVertical="true"/>

</RelativeLayout>